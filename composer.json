{
    "name": "spinen/laravel-mail-assertions",
    "description": "PHPUnit mail assertions for testing email in Laravel.",
    "keywords": [
        "email",
        "phpunit",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "^5.7.26",
        "swiftmailer/swiftmailer": "~5.1|~6.0",
        "phpunit/phpunit": "~4.0|~5.0|~6.0|~7.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "psy/psysh": "^0.5.1",
        "satooshi/php-coveralls": "~1",
        "symfony/thanks": "^1.0",
        "symfony/var-dumper": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\MailAssertions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\MailAssertions\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
