{
    "name": "spinen/laravel-mail-assertions",
    "description": "PHPUnit mail assertions for testing email in Laravel.",
    "keywords": [
        "email",
        "phpunit",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        },
        {
            "name": "Stephen Finney",
            "email": "stephen.finney@spinen.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/container": "~9|~10",
        "illuminate/mail": "~9|~10",
        "swiftmailer/swiftmailer": "~6.2",
        "phpunit/phpunit": "^9.0"
    },
    "require-dev": {
        "egulias/email-validator": "^2.1.16",
        "mockery/mockery": "^1.5.1",
        "psy/psysh": "^0.11.1",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\MailAssertions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\MailAssertions\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
