{
    "name": "spinen/laravel-mail-assertions",
    "description": "PHPUnit mail assertions for testing email in Laravel.",
    "keywords": [
        "email",
        "phpunit",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.5|~6",
        "swiftmailer/swiftmailer": "~6.0",
        "phpunit/phpunit": "~7.0.1|~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1",
        "psy/psysh": "^0.9.9",
        "symfony/thanks": "^1.1",
        "symfony/var-dumper": "~3.0|^4.2"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\MailAssertions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\MailAssertions\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
